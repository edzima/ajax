file = open('dane.txt', 'r').read()
lines = file.split('\n')
dane =[]
for line in lines:
    dane.append(line.split(' '))

print dane

nWierszy = len(dane)


def f1 (wiersz,kolumna,s):
    if(wiersz<nWierszy-1):    
        print dane[wiersz][kolumna]
        print dane[wiersz+1][kolumna]
        print dane[wiersz+1][kolumna+1]
        s = s+ int(dane[wiersz][kolumna])+int(dane[wiersz+1][kolumna])+int(dane[wiersz+1][kolumna+1])
        print s

suma =0
for wiersz in range(0,nWierszy):
    for kolumna in range(0,len(dane[wiersz])):
        f1(wiersz,kolumna, suma)
        

print suma



